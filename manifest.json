{
    "manifest_version": 3,
    "name": "leetcode2github",
    "version": "1.0",
    "description": "Want to upload your LeetCode submissions to GitHub? Why don't you check out out simple plugin!",
    "action": {
        "default_popup": "./popup/home.html",
        "default_icon": "./images/icons/icon.png"
    },
    "icons": {
        "16": "images/icons/icon-16.png",
        "32": "images/icons/icon-32.png",
        "48": "images/icons/icon-48.png",
        "128": "images/icons/icon-128.png"
    }
}
